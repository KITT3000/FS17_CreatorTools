<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<GUI onOpen="onOpen" onClose="onClose" onCreate="onCreate">
   <GuiElement type="bitmap" profile="backgroundImageIngame" />
   <GuiElement type="bitmap" profile="uiElementBg">
      <GuiElement type="bitmap" profile="uiElementMainBg">
         <GuiElement type="bitmap" profile="uiElementBaseHeader">
            <GuiElement type="multiTextOption" profile="headerBoxSelector" onClick="onClickPageSelection" id="pageSelector" focusId="1" focusChangeBottom="2">
               <GuiElement type="button" profile="headerBoxSelectorLeft" />
               <GuiElement type="button" profile="headerBoxSelectorRight" />
               <GuiElement type="text" profile="headerText" text="Paging" />
            </GuiElement>
         </GuiElement>
         <GuiElement type="paging" onPageChange="onPageChange" onPageUpdate="onPageUpdate" id="pagingElement">
            <!-- Command setFieldFruit -->
            <GuiElement type="empty" profile="page" name="setFieldFruit" onCreate="onCreatePageSetFieldFruit">
               <GuiElement type="multiTextOption" profile="multiTextOptionSettings" position="20px 470px" onOpen="onOpenSetFieldFruitField" id="setFieldFruitField" focusId="1_1" focusChangeBottom="1_2">
                  <GuiElement type="button" profile="multiTextOptionLeft" />
                  <GuiElement type="button" profile="multiTextOptionRight" />
                  <GuiElement type="text" profile="multiTextOptionText" />
                  <GuiElement type="text" profile="multiTextOptionTitle" text="$l10n_ui_CT_setFieldFruitField" />
                  <GuiElement type="bitmap" profile="multiTextOptionBg" />
               </GuiElement>
               <GuiElement type="multiTextOption" profile="multiTextOptionSettings" position="20px 390px" onOpen="onOpenSetFieldFruitFruit" id="setFieldFruitFruit" focusId="1_2" focusChangeBottom="1_3" focusChangeTop="1_1">
                  <GuiElement type="button" profile="multiTextOptionLeft" />
                  <GuiElement type="button" profile="multiTextOptionRight" />
                  <GuiElement type="text" profile="multiTextOptionText" />
                  <GuiElement type="text" profile="multiTextOptionTitle" text="$l10n_ui_CT_setFieldFruitFruit" />
                  <GuiElement type="bitmap" profile="multiTextOptionBg" />
               </GuiElement>
               <GuiElement type="multiTextOption" profile="multiTextOptionSettings" position="20px 310px" onOpen="onOpenSetFieldFruitGrowthState" id="setFieldFruitGrowthState" focusId="1_3" focusChangeBottom="1_4" focusChangeTop="1_2">
                  <GuiElement type="button" profile="multiTextOptionLeft" />
                  <GuiElement type="button" profile="multiTextOptionRight" />
                  <GuiElement type="text" profile="multiTextOptionText" />
                  <GuiElement type="text" profile="multiTextOptionTitle" text="$l10n_ui_CT_setFieldFruitGrowthState" />
                  <GuiElement type="bitmap" profile="multiTextOptionBg" />
               </GuiElement>
               <GuiElement type="multiTextOption" profile="multiTextOptionSettings" position="20px 230px" onOpen="onOpenSetFieldFruitFertilizerState" id="setFieldFruitFertilizerState" focusId="1_4" focusChangeBottom="1_5" focusChangeTop="1_3">
                  <GuiElement type="button" profile="multiTextOptionLeft" />
                  <GuiElement type="button" profile="multiTextOptionRight" />
                  <GuiElement type="text" profile="multiTextOptionText" />
                  <GuiElement type="text" profile="multiTextOptionTitle" text="$l10n_ui_CT_setFieldFruitFertilizerState" />
                  <GuiElement type="bitmap" profile="multiTextOptionBg" />
               </GuiElement>
               <GuiElement type="toggleButton2" profile="multiTextOptionSettings" position="20px 150px" id="setFieldFruitPloughingState" focusId="1_5" focusChangeBottom="1_6" focusChangeTop="1_4">
                  <GuiElement type="button" profile="multiTextOptionLeft" />
                  <GuiElement type="button" profile="multiTextOptionRight" />
                  <GuiElement type="text" profile="multiTextOptionText" />
                  <GuiElement type="text" profile="multiTextOptionTitle" text="$l10n_ui_CT_setFieldFruitPloughingState" />
                  <GuiElement type="bitmap" profile="multiTextOptionBg" />
               </GuiElement>
               <GuiElement type="toggleButton2" profile="multiTextOptionSettings" position="20px 70px" id="setFieldFruitBuyField" focusId="1_6" focusChangeTop="1_5">
                  <GuiElement type="button" profile="multiTextOptionLeft" />
                  <GuiElement type="button" profile="multiTextOptionRight" />
                  <GuiElement type="text" profile="multiTextOptionText" />
                  <GuiElement type="text" profile="multiTextOptionTitle" text="$l10n_ui_CT_setFieldFruitBuyField" />
                  <GuiElement type="bitmap" profile="multiTextOptionBg" />
               </GuiElement>
            </GuiElement>
            <!-- Command setFieldGround -->
            <GuiElement type="empty" profile="page" name="setFieldGround" onCreate="onCreatePageSetFieldGround" />
         </GuiElement>
         <GuiElement type="bitmap" profile="ingameMenuHelpRowBg" id="panelHelpBox" visible="false">
            <GuiElement type="bitmap" profile="ingameMenuHelpRowIcon" />
            <GuiElement type="text" profile="ingameMenuHelpRowText" id="panelHelpBoxText" text="" />
         </GuiElement>
         <GuiElement type="flowLayout" profile="ingameMenuPageStateBg" id="pageStateBox">
            <GuiElement type="bitmap" profile="ingameMenuPageState" id="pageStateElement" />
         </GuiElement>
         <GuiElement type="flowLayout" profile="buttonBox" id="buttonsPC">
            <GuiElement type="button" profile="buttonBack" size="499px 40px" text="$l10n_button_back" onClick="onClickBack" id="backButton" focusId="2" focusChangeRight="3" focusChangeTop="1" />
            <GuiElement type="button" profile="buttonOK" size="499px 40px" text="$l10n_ui_execute" onClick="onClickActivate" id="activateButton" focusId="3" focusChangeLeft="2" focusChangeTop="1" focusInit="onOpen" />
         </GuiElement>
      </GuiElement>
   </GuiElement>
</GUI>